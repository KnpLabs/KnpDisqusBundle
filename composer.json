{
    "name": "knplabs/knp-disqus-bundle",
    "type": "symfony-bundle",
    "description": "",
    "keywords": ["comments", "forum", "disqus"],
    "homepage": "http://github.com/KnpLabs/KnpDisqusBundle",
    "license": "MIT",

    "authors": [
        {
            "name": "KnpLabs Team",
            "homepage": "http://knplabs.com"
        },
        {
            "name": "Symfony Community",
            "homepage": "http://github.com/KnpLabs/KnpDisqusBundle/contributors"
        }
    ],

    "require": {
        "php": ">=7.2.5",
        "ext-json": "*",
        "symfony/http-client": "^4.4 || ^5.3 || ^6.0"
    },

    "require-dev": {
        "phpstan/phpstan": "^1.2",
        "phpunit/phpunit": "^8.5 || ^9.5",
        "symfony/framework-bundle": "^4.4 || ^5.3 || ^6.0",
        "twig/twig": "^2.10|^3.0"
    },

    "autoload": {
        "psr-4": { "Knp\\Bundle\\DisqusBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Knp\\Bundle\\DisqusBundle\\Tests\\": "tests/" }
    },

    "scripts": {
        "test": "vendor/bin/phpunit"
    },

    "extra": {
        "branch-alias": {
            "dev-master": "2.1.x-dev"
        }
    }
}
